#include<bits/stdc++.h>
using namespace std;

string vowel(string S){
    // your code goes here
   // sort(S.begin(),S.end());
    string str;
    for(int i=0;i<S.length();i++){
        if(S[i]=='a' || S[i]=='e'||S[i]=='i' || S[i]=='o' || S[i]=='u'){
            str.push_back(S[i]);
        }
    }
    return str;
    
} 
